{
  "manifest_version": 3,
  "name": "Life Circles",
  "version": "1.0",
  "description": "Track your life in circles.",
  "permissions": ["storage"],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Set Your Age"
  }
}