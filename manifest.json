{
  "manifest_version": 3,
  "name": "Life Circles",
  "version": "1.0",
  "description": "Track your life in circles.",
  "permissions": ["storage"],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Set Your Age",
    "default_icon": {
      "16": "images/logo_16x16.png",
      "48": "images/logo_48x48.png",
      "128": "images/logo_128x128.png"
    }
  }
}